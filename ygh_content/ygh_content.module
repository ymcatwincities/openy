/**
 * Prepare data for paragraphs migration.
 *
 * @param array $ids
 *   Array with IDs.
 *
 * @return array
 *   Array for process plugin.
 */
function ygh_content_prepare_data(array $ids) {
  return [
    'entity' => [
      'target_id' => $ids[0],
      'target_revision_id' => $ids[1],
    ],
  ];
}
