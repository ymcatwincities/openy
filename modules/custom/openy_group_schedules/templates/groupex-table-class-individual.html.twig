{#
/**
 * @file
 * GroupEx Pro class template.
 *
 * Available variables:
 * - class: array of single class variables;
 *
 * @ingroup themeable
 */
#}

<tr>
  <td class="time-duration">
    <span class="icon icon-clock"></span>{{ class.time }}  <span class="duration">{{ class.duration }}</span>
  </td>
  <td>
    <div>
      {% if 'class' in used_filters %}
        {{ class.name }}
      {% else %}
        <a class="use-ajax" href="{{ class.class_link }}">{{ class.name }}</a>
      {% endif %}
    </div>
    <div>{{ class.studio }}</div>
  </td>
  <td class="instructor">
    <span class="icon icon-user"></span><a class="use-ajax" href="{{ class.instructor_link }}">{{ class.instructor|raw }}</a>
  </td>
  <td class="ical">
    <span class="addtocalendar atc-style-blue">
      <var class="atc_event">
        <var class="atc_date_start">{{ class.calendar.atc_date_start }}</var>
        <var class="atc_date_end">{{ class.calendar.atc_date_end }}</var>
        <var class="atc_timezone">{{ class.calendar.atc_timezone }}</var>
        <var class="atc_title">{{ class.calendar.atc_title }}</var>
        <var class="atc_description">{{ class.calendar.atc_description }}</var>
        <var class="atc_location">{{ class.calendar.atc_location }}</var>
        <var class="atc_organizer">{{ class.calendar.atc_organizer }}</var>
      </var>
    </span>
  </td>
</tr>

