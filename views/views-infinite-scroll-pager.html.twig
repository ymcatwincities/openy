{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}
{% if items.next %}
  <ul{{ attributes }}>
    <li class="pager__item">
      <a class="button btn btn-primary" href="{{ items.next.href }}" title="{{ 'Go to next page'|t }}" rel="next">{{ options.button_text }}</a>
    </li>
  </ul>
{% endif %}
