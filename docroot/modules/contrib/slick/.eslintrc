{
  // This file extends .eslintrc at drupal root.
  // Located at /modules/contrib/slick, run:
  // eslint . -o ../../eslint/slick.html -f html
  "extends": "../../../.eslintrc",
  "globals": {
    "Blazy": true
  },
  "rules": {
    "consistent-this": [0, "me"],
    "max-params": 0,
    "id-length": [1, {"min": 1}],
    // Warnings.
    "max-nested-callbacks": [1, 4]
  }
}
