{#
/**
 * @file
 * Display retention campaign winners form with branches.
 */
#}

<header class="compain-header">
  <div class="compain-header--content">
    <div class="container">
      <div class="logo">
        <img alt="logo" src="{{ base_path }}themes/custom/ymca/prototypes/ysr_2017/img/logo-phoenix-light.svg">
      </div>

      <div class="content">
        <div class="title">{{ 'Y RECHARGE CHALLENGE'|t }}</div>
        <div class="sub-title">{{ 'Lucky Winners'|t }}</div>
      </div>
      <p style="color: white; font-weight: bold;">Y Bucks will be available for pick up in branch on April 1</p>
    </div>
  </div>
</header>
<div class="compain-form-winners">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <div class="form-item form-type-select">
          <select id="edit-winners" class="form-select"
                  data-ng-model="selectedBranch"
                  data-ng-options="br.name for br in branches track by br.id"
                  data-ng-change="update()">
          </select>
        </div>
      </div>
    </div>
  </div>
</div>
