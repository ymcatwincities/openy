/**
 * Add field_camp_menu field to Camp and Landing CT.
 */
function openy_location_core_update_8001() {
  $features = [
    'openy_node_camp',
    'openy_node_landing',
  ];
  foreach ($features as $feature) {
    \Drupal::service('config_import.features_importer')->import($feature);
  }
}
