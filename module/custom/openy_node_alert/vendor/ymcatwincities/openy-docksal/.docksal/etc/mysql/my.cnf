[mysqld]
# Memory settings.
key_buffer_size = 512M
max_allowed_packet = 1024M
table_open_cache = 512
sort_buffer_size = 2M
read_buffer_size = 2M
read_rnd_buffer_size = 8M
myisam_sort_buffer_size = 64M
thread_cache_size = 8
query_cache_size = 16M
max_connections = 151
# Other settings.
wait_timeout = 500
