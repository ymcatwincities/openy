{% extends "page.html.twig" %}

{% block pagecontent %}
  <section class="page-header page-title title-only">
    <div class="container title-container">
      <div class="row">
        <div class="col-sm-12 header-copy">
          <h1>{{ attribute(page, '#title') }}</h1>
        </div>
      </div>
    </div>
  </section>
  <section class="content">
    <div class="blog-page container">
      {{ page.content }}
    </div>
  </section>
{% endblock %}
